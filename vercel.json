{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": ""
        },
        {
          "key": "X-Robots-Tag",
          "value": "all"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
 
  "trailingSlash": false
}
